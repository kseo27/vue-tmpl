{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@@/*": [
				"./*"
			],
			"@components/*": [
				"./src/components/*"
			],
			"@composables/*": [
				"./src/composables/*"
			],
			"@const/*": [
				"./src/common/constants/*"
			],
			"@services/*": [
				"./src/services/*"
			],
			"@store/*": [
				"./src/store/*"
			],
			"@utils/*": [
				"./src/common/utils/*"
			],
			"@views/*": [
				"./src/views/*"
			]
		}
	},
	"exclude": [
		"node_modules",
		"dist",
		"public"
	]
}